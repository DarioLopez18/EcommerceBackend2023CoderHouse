<div class="container mt-4">
  <h2>Subir Archivos</h2>
  <form
    action="/api/users/{{uid}}/documents"
    method="post"
    enctype="multipart/form-data"
  >

    <div class="form-group">
      <label for="profileImage">Imagen de Perfil: </label>
      <input
        type="file"
        class="form-control-file"
        name="profileImage"
        id="profileImage"
        accept=".jpg,.jpge"
      />
    </div>

    <div class="form-group">
      <label for="documentDNI">Documento Nacional de identidad:</label>
      <input
        type="file"
        class="form-control-file"
        name="documentDNI"
        id="documentDNI"
        accept=".pdf, .doc, .docx, .jpg, ,jpge"
      />
    </div>

    <div class="form-group">
      <label for="comprobanteDomicilio">Comprobante domicilio:</label>
      <input
        type="file"
        class="form-control-file"
        name="comprobanteDomicilio"
        id="comprobanteDomicilio"
        accept=".pdf, .doc, .docx,.jpg, ,jpge"
      />
    </div>

    <div class="form-group">
      <label for="comprobanteEstadoCuenta">Comprobante Estado cuenta:</label>
      <input
        type="file"
        class="form-control-file"
        name="comprobanteEstadoCuenta"
        id="comprobanteEstadoCuenta"
        accept=".pdf, .doc, .docx,.jpg, ,jpge"
      />
    </div>

    <button type="submit" class="btn btn-primary" style="max-width:50%">Subir
      Archivos</button>
  </form>
</div>

